{
    "name": "lukasbableck/contao-vcard-bundle",
    "type": "contao-bundle",
	"description": "This bundle allows you to create vCards in Contao.",
	"keywords": [
		"vcf",
		"contact",
		"vcard",
		"Visitenkarte",
		"business card"
	],
    "license": "LGPL-3.0-or-later",
	"authors": [
		{
			"name": "Lukas Bableck",
			"email": "lukas@bableck.dev",
			"homepage": "https://bableck.dev"
		}
	],
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^5.3",
        "mvo/contao-group-widget": "^1.5",
        "sabre/vobject": "^4.5"
    },
	"conflict": {
		"contao/manager-plugin": "<2.0 || >=3.0"
	},
	"require-dev": {
		"contao/manager-plugin": "^2.0",
		"friendsofphp/php-cs-fixer": "^3.86",
        "vincentlanglet/twig-cs-fixer": "^3.9"
	},
    "autoload": {
        "psr-4": {
            "Lukasbableck\\ContaoVcardBundle\\": "src/"
        }
    },
	"extra": {
		"contao-manager-plugin": "Lukasbableck\\ContaoVcardBundle\\ContaoManager\\Plugin"
	}
}
